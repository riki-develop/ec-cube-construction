{% extends 'default_frame.twig' %}

{% block main %}

<div class="row max-width-660 mx-sm-auto">
  <div class="col">
    <h1 class="h4 py-2 my-2 border-bottom">{{ 'パスワードの再発行(メール送信)'|trans }}</h1>

    <h2 class="h5">{{ 'パスワード再発行メールの送信が完了しました。'|trans }}</h2>
